package hello;

import java.util.Scanner;

public class CompareBigSmall {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		int x=0;
		int y=0;
		int z=0;
		System.out.println("请输入两个数:");
		x = in.nextInt();
		y = in.nextInt();
		z =	in.nextInt();
		
		int max = 0;
		if(x>y)
		{
			if(x>z)
			{
				max=x;
			}else
			{
				max=z;
			}
		}
			else
			{
				if(y>z)
				{
					max = y;
				}
				else
				{
					
				}
			}
			
		System.out.println(max);

	}

}
