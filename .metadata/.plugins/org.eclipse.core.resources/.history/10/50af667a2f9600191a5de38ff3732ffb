package hello;

import java.util.Scanner;

public class World {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int foot;
		int inch;
		
		Scanner in = new Scanner(System.in);
		foot = in.nextInt();
		inch = in.nextInt();
		System.out.println(foot+inch/12+0.3048);

	}

}
