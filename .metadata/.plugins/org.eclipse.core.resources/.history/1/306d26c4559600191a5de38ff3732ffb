package hello;

import java.util.Scanner;

public class CalcMath {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int number = in.nextInt();
		int count = 0;
		
		//只要条件是满足的 就做括号后面的内容
		while(number>0)
		{
			number = number/10;
			count = count+1;
		}
		System.out.println(count);

	}

}
