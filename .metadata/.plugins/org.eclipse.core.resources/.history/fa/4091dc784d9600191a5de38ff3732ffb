package hello;

import java.util.Scanner;

public class CompareBigSmall {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner in = new Scanner(System.in);
		int x=0;
		int y=0;
		System.out.println();
		int x = in.nextInt();
		int y = in.nextInt();
		int max = 0;
		if(x>y)
			max=x;
		else
			max = y;
		System.out.println(max);

	}

}
