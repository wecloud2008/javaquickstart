package hello;

import java.util.Scanner;

public class LoopDoWhile {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in= new Scanner(System.in);
		int count = 10;
		do
		{
			System.out.println(count);
			count = count-1;			
		}while(count>0);
		System.out.println(count);
		System.out.println("发射!");
		//变量->算法->流程图->程序

	}

}
